extends layout.pug
include utils.pug

block content
    form(action="/user/login" method="post")
        input(type="hidden", name="_csrf" value=token)
        +field("Email Address", "emailAddress", "EX: example@example.com", "email")
        +field("Password", "password", "EX: aA1!", "password")
        button(type="submit") Submit
